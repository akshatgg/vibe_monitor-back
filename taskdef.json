{
  "family": "vm-api",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::337502598894:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::337502598894:role/ecsAppTaskRole",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "337502598894.dkr.ecr.us-west-1.amazonaws.com/vm-api:REPLACED_BY_CI",
      "essential": true,
      "portMappings": [{"containerPort": 8000, "protocol":"tcp"},{"containerPort": 4317, "protocol":"tcp"}],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/vm-api",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {"name":"ENVIRONMENT","value":"ENVIRONMENT_PLACEHOLDER"},
        {"name":"SUPABASE_URL","value":"SUPABASE_URL_PLACEHOLDER"},
        {"name":"SUPABASE_ANON_KEY","value":"SUPABASE_ANON_KEY_PLACEHOLDER"},
        {"name":"SUPABASE_SERVICE_KEY","value":"SUPABASE_SERVICE_KEY_PLACEHOLDER"},
        {"name":"SUPABASE_DATABASE_URL", "value":"SUPABASE_DATABASE_URL_PLACEHOLDER"}
        {"name":"API_BASE_URL","value":"API_BASE_URL_PLACEHOLDER"},
        {"name":"CLICKHOUSE_HOST","value":"CLICKHOUSE_HOST_PLACEHOLDER"},
        {"name":"CLICKHOUSE_PASSWORD","value":"CLICKHOUSE_PASSWORD_PLACEHOLDER"},
        {"name":"CLICKHOUSE_DATABASE","value":"CLICKHOUSE_DATABASE_PLACEHOLDER"},
        {"name":"CLICKHOUSE_SECURE","value":"CLICKHOUSE_SECURE_PLACEHOLDER"},
        {"name":"CLICKHOUSE_PORT","value":"CLICKHOUSE_PORT_PLACEHOLDER"},
        {"name":"CLICKHOUSE_USER","value":"CLICKHOUSE_USER_PLACEHOLDER"},
        {"name":"GOOGLE_CLIENT_ID","value":"GOOGLE_CLIENT_ID_PLACEHOLDER"},
        {"name":"GOOGLE_CLIENT_SECRET","value":"GOOGLE_CLIENT_SECRET_PLACEHOLDER"},
        {"name":"GOOGLE_REDIRECT_URI","value":"GOOGLE_REDIRECT_URI_PLACEHOLDER"},
        {"name":"ALLOWED_ORIGINS","value":"ALLOWED_ORIGINS_PLACEHOLDER"}
      ]
    }
  ]
}
